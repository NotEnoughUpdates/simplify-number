{
  "name": "@notenoughupdates/simplify-number",
  "version": "1.0.1",
  "description": "Simplify Number",
  "main": "./dist/index.js",
  "repository": "https://github.com/rohmanhm/simplify-number.git",
  "typings": "./dist/index.d.ts",
  "author": "MH Rohman",
  "license": "MIT",
  "private": false,
  "scripts": {
    "build": "yarn tsc",
    "build:no-emit": "yarn tsc --noEmit",
    "test": "tsc && ava",
    "prepare": "yarn build",
    "prepublishOnly": "yarn build:no-emit"
  },
  "devDependencies": {
    "ava": "^3.15.0",
    "ts-node": "^10.4.0",
    "tslint": "^6.1.3",
    "tslint-config-standard": "^9.0.0",
    "typescript": "^4.5.2"
  },
  "ava": {
    "files": [
      "test.ts"
    ],
    "verbose": true,
    "compileEnhancements": false,
    "extensions": [
      "ts"
    ],
    "require": [
      "ts-node/register"
    ]
  },
  "files": [
    "src/",
    "dist/",
    "README.md",
    "LICENSE",
    "tsconfig.json",
    "yarn.lock"
  ]
}